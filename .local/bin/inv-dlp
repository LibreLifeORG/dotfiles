#!/bin/sh

yt-dlp --cookies $HOME/doxs/passwords/cookies.txt --no-playlist --extractor-args "youtube:player_js_version=actual" -o "%(channel)s - %(title)s - [%(id)s].%(ext)s" $(echo $1 | sed 's|inv.nadeko.net|www.youtube.com|')

